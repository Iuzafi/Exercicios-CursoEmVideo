algoritmo "semnome"
var
   cont, n, r: inteiro
inicio
      cont <- 1
      Escreva ("Digite o numero a ser multiplicado: ")
      Leia (N)
      Repita
            R <- N * cont
            Escreval (N, " x", cont, " = ", R)
            Cont <- cont + 1
      Ate (Cont > 10)
fimalgoritmo
