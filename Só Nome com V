algoritmo "SÃ³ nome com V"
var
   nome: caractere
   soV: vetor [1..10] de caractere
   C, Tot: inteiro
inicio
      Para c de 1 ate 10 Faca
           Escreva ("Digite um nome: ")
           Leia (nome)
           Se (copia(maiusc(nome), 1, 1) = "V") Entao
              tot <- tot + 1
              soV[c] <- nome
           FimSe
      FimPara
      LimpaTela
      
      Escreval ("LISTAGEM FINAL")
      Para c de 1 ate Tot Faca
           Escreval (soV[c])
      FimPara
fimalgoritmo
